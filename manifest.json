{
  "manifest_version": 2,
  "name": "Wickedizer",
  "description": "Makes the web wicked",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["myscript.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon1.png"
  }
}